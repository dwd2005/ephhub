{
  "name": "datahelper-pc",
  "version": "0.1.0",
  "description": "Local data management MVP built with Electron + React",
  "main": "electron/main.js",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently -k --success=all \"npm:dev:renderer\" \"npm:dev:electron\"",
    "dev:renderer": "vite --host",
    "dev:electron": "set VITE_DEV_SERVER_URL=http://localhost:5173&& electron .",
    "build": "vite build",
    "lint": "eslint . --ext .ts,.tsx,.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "antd": "^5.20.0",
    "@ant-design/icons": "^5.4.0",
    "sqlite3": "^5.1.7",
    "chokidar": "^3.6.0",
    "dayjs": "^1.11.13",
    "fs-extra": "^11.2.0",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "concurrently": "^8.2.2",
    "electron": "^30.0.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.8"
  }
}
